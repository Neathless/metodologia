#include <stdbool.h>
#include <stdio.h>

int main() {
  int n;
  printf("Ingrese un número entero positivo: ");
  scanf("%d", &n);

  printf("Números primos menores o iguales a %d:\n", n);
  for (int i = 2; i <= n; i++) {
    bool esPrimo = true;
    for (int j = 2; j * j <= i; j++) {
      if (i % j == 0) {
        esPrimo = false;
        break;
      }
    }
    if (esPrimo) {
      printf("%d ", i);
    }
  }
  printf("\n");

  return 0;
}
