#include <stdio.h>

int main() {
float numero;
int contador;
contador = 0;
do {
    printf ("Ingresa un numero (pulsa 0 para terminar):");
    scanf ("%f", &numero);
 
if (numero > 0){
    contador += 1;
}
}
while (numero !=0);
printf ("la cantidad de numeros ingresados fue: %d", contador);
    return 0;
}
