//Algoritmo #1
#include <stdio.h>

int main(void) {
  int num1, num2;
  printf("Ingresa el primer numero:\n");
  scanf ("%d", &num1);
  printf ("Ingresa el segundo numero:\n");
  scanf ("%d", &num2);
  if (num1 > num2) {
    printf ("El numero mayor es: %d", num1);
  }
  else if (num1 == num2) {
    printf ("Los numeros son iguales");
  }
  else
    printf ("El numero mayor es: %d", num2);
  return 0;
}
